{
  "name": "discord.ts-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "prepare": "husky install",
    "type-check": "turbo run type-check"
  },
  "packageManager": "pnpm@7.12.2",
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.1.1",
    "turbo": "^1.7.0"
  }
}
